{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "None",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Proxy": {
    "EnableHttp": true,
    "HttpPort": 8080,
    "EnableHttps": true,
    "HttpsPort": 8443,
    "EnableMtls": true,
    "MtlsPort": 9443,
    "SleepDurationMs": 5000,
    "ServerCertificatePath": "../../certs/generated/server.pfx",
    "ServerCertificatePassword": "changeit",
    "TrustedCaPath": "../../certs/generated/ca.cert.pem",
    "OutboundClientCertificatePath": "../../certs/generated/client.pfx",
    "OutboundClientCertificatePassword": "changeit",
    "UpstreamUrl": "",
    "PassConnectionClose": false
  }
}
